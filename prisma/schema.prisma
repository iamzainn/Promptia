


generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model User {
  id        String    @id @default(uuid()) @map("_id")
  clerkId   String @unique
  attributes Json
  prompts   Prompt[] 
}

model Prompt {
  id       String   @id @default(uuid()) @map("_id")
  createdAt DateTime @default(now())
  user     User    @relation(fields: [userId], references: [clerkId])
  userId   String  
}




